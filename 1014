using System; 

class URI {

    static void Main(string[] args) { 

        int X;
            double Y,KML;

            
            X = Convert.ToInt32(Console.ReadLine());
            Y = Convert.ToDouble(Console.ReadLine());

            KML = X / Y;

            
            Console.WriteLine("{0:0.000} km/l", KML);

    }

}
